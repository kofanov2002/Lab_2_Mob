            android:layout_marginTop="176dp" android:layout_marginStart="148dp"/>
    <CheckBox
            android:text="CheckBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/checkBox"
            app:layout_constraintTop_toBottomOf="@+id/button" app:layout_constraintStart_toStartOf="@+id/textView3"
            android:layout_marginTop="104dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="59dp"
            android:layout_height="14dp" android:id="@+id/textView4" tools:layout_editor_absoluteY="597dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.22"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2" tools:layout_editor_absoluteY="581dp"
            tools:layout_editor_absoluteX="272dp"/>
